{
  "name": "@orpc/client",
  "type": "module",
  "version": "0.43.0",
  "license": "MIT",
  "homepage": "https://orpc.unnoq.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unnoq/orpc.git",
    "directory": "packages/client"
  },
  "keywords": [
    "unnoq",
    "orpc"
  ],
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs"
      },
      "./openapi": {
        "types": "./dist/openapi/index.d.mts",
        "import": "./dist/openapi/index.mjs",
        "default": "./dist/openapi/index.mjs"
      },
      "./rpc": {
        "types": "./dist/rpc/index.d.mts",
        "import": "./dist/rpc/index.mjs",
        "default": "./dist/rpc/index.mjs"
      },
      "./fetch": {
        "types": "./dist/adapters/fetch/index.d.mts",
        "import": "./dist/adapters/fetch/index.mjs",
        "default": "./dist/adapters/fetch/index.mjs"
      }
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./openapi": "./src/openapi/index.ts",
    "./rpc": "./src/rpc/index.ts",
    "./fetch": "./src/adapters/fetch/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:watch": "pnpm run build --watch",
    "type:check": "tsc -b"
  },
  "dependencies": {
    "@orpc/shared": "workspace:*",
    "@orpc/standard-server": "workspace:*",
    "@orpc/standard-server-fetch": "workspace:*"
  },
  "devDependencies": {
    "zod": "^3.24.1"
  }
}
